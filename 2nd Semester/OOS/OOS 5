#include<iostream>
using namespace std;

class Point {
private:
    double x, y;  // Private Datenmember fÃ¼r die x- und y-Koordinaten des Punktes.

public:
    // Setter-Methode, um die x/y-Koordinate zu setzen
    void set_x(double x) {
        this->x = x;  
    }

    void set_y(double y) {
        this->y = y;  
    }
    // Getter-Methode, um die x/y-Koordinate abzurufen
    double get_x() {
        return x;  
    }

    double get_y() {
        return y;  
    }
    // Ausgabe der x/y-Koordinate
    void print() {
        cout << "print-Methode:" << endl;
        cout << "X = " << get_x() << endl; 
        cout << "Y = " << get_y() << endl;  
    }
};


int main(int argc, char* argv[]) {
    Point p;
    p.set_x(1);
    p.set_y(10);
    p.print();
    p.set_x(5);
    cout << "X ohne print(): " << p.get_x() << endl;
    cout << "Y ohne print(): " << p.get_y() << endl;
    return 0;
}
